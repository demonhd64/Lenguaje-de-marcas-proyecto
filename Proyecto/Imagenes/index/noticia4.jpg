										<script>
											function articulo_chequear_newsletter() {
												var fallo = false;
												var alerta = "";
												if ($("#articulo-newsletter-email").val().length == 0) {
													alerta = "Introduce una dirección de email correcta";
													fallo = true;
												}
												if ($("#articulo-newsletter-edicion-id").val() == "0") {
													alerta = "Selecciona una edición";
													fallo = true;
												}
												if (!$("#articulo-newsletter-acepto").is(":checked")) {
													alerta = "Acepta las condiciones para poder inscribirte";
													fallo = true;
												}
												if (fallo) {
													alert(alerta);
													return false;
												}
												if (!fallo) {
													articulo_enviar_formulario_newsletter();
												}
											}
											$("#edicion_id").val(0);

											function articulo_enviar_formulario_newsletter() {
												var email = $("#articulo-newsletter-email").val();
												var edicion = $("#articulo-newsletter-edicion-id").val();
												$.ajax({
													type: "POST",
													url: `${PROTOCOL}${DOMAIN}/web/modulos/newsletter_enviar.php`,
													data: {
														'form-newsletter': email,
														'edicion_id': edicion
													},
													success: function(data) {
														$("#articulo-newsletter-email").val("");
														$("#articulo-newsletter-edicion-id").val("");
														if (data == "OK") {
															$('#articulo-newsletter-form').hide();
															$('#articulo_newsletter_ok').show();
														} else if (data == "KO") {
															$('#articulo-newsletter-form').hide();
															$('#articulo_newsletter_ko').show();
														} else if (data == "KO EMAIL") {
															$('#articulo-newsletter-form').hide();
															$('#articulo_newsletter_ko_email').show();
														} else {
															alert(data);
														}
													}
												});
											}
										</script>
